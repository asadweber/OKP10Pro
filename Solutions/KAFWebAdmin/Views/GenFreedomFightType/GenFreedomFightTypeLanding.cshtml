
@{
    ViewBag.Title = KAF.MsgContainer._gen_freedomfighttype.freedomfighttypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenFreedomFightType/GenFreedomFightTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md" id="btnNewGenFreedomFightType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenFreedomFightTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._gen_freedomfighttype.freedomfight</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenFreedomFightTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenFreedomFightTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-GenFreedomFightTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenFreedomFightTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenFreedomFightTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenFreedomFightTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenFreedomFightType/LandingGenFreedomFightType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenFreedomFightType/NewGenFreedomFightType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenFreedomFightType/EditGenFreedomFightType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenFreedomFightType/DetailGenFreedomFightType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



