
@{
    ViewBag.Title = KAF.MsgContainer._gen_building.buildingList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenBuilding/GenBuildingNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenBuilding">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenBuildingDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_building.buildingname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenBuildingNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBuildingNew">
        </div>
    </div>
</div>

<div id="modal-container-GenBuildingEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBuildingEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenBuildingDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBuildingDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBuilding/LandingGenBuilding.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBuilding/NewGenBuilding.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBuilding/EditGenBuilding.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBuilding/DetailGenBuilding.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



