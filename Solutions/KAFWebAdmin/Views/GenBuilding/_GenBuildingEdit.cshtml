@model KAF.BusinessDataObjects.gen_buildingEntity

<style>
        .form-horizontal .form-group {
            margin-right: 10px;
            margin-left: 10px;
        }
    </style>

@using (Ajax.BeginForm("", "", null, new AjaxOptions { HttpMethod = "Post", OnSuccess = "", LoadingElementId = "divProgressBar" }, new { @id = "frmGen_BuildingEdit", @class = "form-horizontal" }))
{
    HtmlHelper.UnobtrusiveJavaScriptEnabled = true;
    HtmlHelper.ClientValidationEnabled = true;
    @Html.AntiForgeryToken()
    
    <div class="panel panel-primary list-panel" id="list-panel">
        <div class="panel-heading list-panel-heading">
            @KAF.MsgContainer._gen_building.buildingUpdate
        </div>
        
        <div class="panel-body">
            <div class="modal-body">
                <div class="form-horizontal container">
                    <div class="row">
                        @Html.HiddenFor(m => m.buildingid)
                        @Html.HiddenFor(m => m.strModelPrimaryKey)
                    </div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.buildingname, htmlAttributes: new { @class = "labelNormal required " })
								 @Html.EditorFor(model => model.buildingname, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.buildingname, "", new { @class = "text-danger" })
							</div>
						</div>
					</div>
                     <div class="row">
                         <div class="col-md-6">
                             @{Html.RenderAction("LoadGovCityCascadeSearch", "Common", new
                                 {
                                     selectidForGov = "kwgovid",
                                     minimumInputLengthForGov = 0,
                                     preloadedGovData = ViewBag.preloadedGen_GovCity,
                                     multipleForGov = false,
                                     isReadOnlyForGov = false,
                                     isRequiredForGov = true,

                                     selectidForCity = "kwareaid",
                                     minimumInputLengthForCity = 0,
                                     preloadedCityData = ViewBag.preloadedGen_GovArea,
                                     multipleForCity = false,
                                     isReadOnlyForCity = false,
                                     isRequiredForCity = true
                                 });}
                         </div>
                     </div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.kwblockno, htmlAttributes: new { @class = "labelNormal required " })
								 @Html.EditorFor(model => model.kwblockno, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.kwblockno, "", new { @class = "text-danger" })
							</div>
						</div>
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.kwstreet, htmlAttributes: new { @class = "labelNormal required " })
								 @Html.EditorFor(model => model.kwstreet, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.kwstreet, "", new { @class = "text-danger" })
							</div>
						</div>
					</div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.kwhouseno, htmlAttributes: new { @class = "labelNormal required " })
								 @Html.EditorFor(model => model.kwhouseno, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.kwhouseno, "", new { @class = "text-danger" })
							</div>
						</div>
						 <div class="col-md-6">
							 <div class="form-group">
							 @Html.LabelFor(model => model.isactive, htmlAttributes: new { @class = "labelNormal  " })
							 @Html.EditorFor(model => model.isactive, new { htmlAttributes = new {  @class = "form-control" } })
							 @Html.ValidationMessageFor(model => model.isactive, "", new { @class = "text-danger" })
							</div>
						</div>
					</div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.remarks, htmlAttributes: new { @class = "labelNormal  " })
								 @Html.EditorFor(model => model.remarks, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.remarks, "", new { @class = "text-danger" })
							</div>
						</div>
					</div>
                    <div class="btn-toolbar">
                        <button type="submit" id="btnUpdateGenBuilding" class="btn btn-primary btn-md btnUpdateGenBuilding"><i class="fa fa-edit"> </i> @KAF.MsgContainer._Common._btnUpdate</button>
                        <button type="submit" id="btnModalCloseEdit" class="btn btn-danger btn-md cancel"><i class="fa fa-arrow-circle-left"> </i> @KAF.MsgContainer._Common._btnCancel</button>
                    </div> 
                </div>
            </div>
        </div>
    </div>
}


<script>
//filecontrol addons




</script>






