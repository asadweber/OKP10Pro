
@{
    ViewBag.Title = "SignOut";
    Layout = "~/Views/Shared/_Login.cshtml";
}

<META Http-Equiv="Cache-Control" Content="no-cache">
<META Http-Equiv="Pragma" Content="no-cache">
<META Http-Equiv="Expires" Content="0″>
<div class="container">
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-xs-12 col-md-6">
            <div class="panel with-nav-tabs panel-info">
                <div class="panel-heading">
                    <ul class="nav nav-tabs"></ul>
                </div>
                <div class="panel-body">
                    <div class="tab-content">
                        <form id="LoginForm" data-toggle="validator">

                            <div id="login" class="tab-pane fade in active register">
                                <div class="container-fluid">
                                    <div class="row">
                                        <h2 class="text-center" style="color: #5cb85c;"> <strong> Sign Out Successfully </strong></h2><hr />
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="input-group">
                                                        You have been sign out from the system successfully. 
                                                        <br />
                                                        <a href="http://localhost:1892/Login/Index" class="btn btn-default btn-flat">Login</a>
                                                    </div>
                                                </div>
                                            </div>
                                            <hr />
                                        </div>
                                    </div>
                                </div>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>

