
@{
    ViewBag.Title = KAF.MsgContainer._gen_batch.batchList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenBatch/GenBatchNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenBatch">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenBatchDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_batch.batchname</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenBatchNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenBatchNew">
        </div>
    </div>
</div>

<div id="modal-container-GenBatchEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenBatchEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenBatchDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenBatchDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBatch/LandingGenBatch.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBatch/NewGenBatch.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBatch/EditGenBatch.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBatch/DetailGenBatch.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



