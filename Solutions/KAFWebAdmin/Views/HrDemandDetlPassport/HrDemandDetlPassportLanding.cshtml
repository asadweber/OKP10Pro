
@{
    ViewBag.Title = KAF.MsgContainer._hr_demanddetlpassport.demanddetlpassportList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrDemandDetlPassport/HrDemandDetlPassportNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md" id="btnNewHrDemandDetlPassport">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrDemandDetlPassportDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._hr_demanddetlpassport.letterno</th>
                            <th>@KAF.MsgContainer._hr_demanddetlpassport.letterdate</th>
                            <th>@KAF.MsgContainer._hr_demanddetlpassport.passportrecvdate</th>
                            <th>Total Person</th>
                            <th class="hdrletterstat ">Status</th>
                            <th style="width:300px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrDemandDetlPassportNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrDemandDetlPassportNew">
        </div>
    </div>
</div>

<div id="modal-container-HrDemandDetlPassportEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrDemandDetlPassportEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrDemandDetlPassportDetail" class="modal fade hidden-print reportmodal" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrDemandDetlPassportDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDemandDetlPassport/LandingHrDemandDetlPassport.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDemandDetlPassport/NewHrDemandDetlPassport.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDemandDetlPassport/EditHrDemandDetlPassport.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDemandDetlPassport/DetailHrDemandDetlPassport.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



