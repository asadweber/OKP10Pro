
@{
    ViewBag.Title = KAF.MsgContainer._gen_platoon.platoonList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenPlatoon/GenPlatoonNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenPlatoon">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenPlatoonDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_platoon.name</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenPlatoonNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenPlatoonNew">
        </div>
    </div>
</div>

<div id="modal-container-GenPlatoonEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenPlatoonEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenPlatoonDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenPlatoonDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPlatoon/LandingGenPlatoon.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPlatoon/NewGenPlatoon.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPlatoon/EditGenPlatoon.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPlatoon/DetailGenPlatoon.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



