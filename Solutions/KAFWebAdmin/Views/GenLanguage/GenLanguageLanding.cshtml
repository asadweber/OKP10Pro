
@{
    ViewBag.Title = KAF.MsgContainer._gen_language.languageList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenLanguage/GenLanguageNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenLanguage">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenLanguageDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_language.languagename</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenLanguageNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenLanguageNew">
        </div>
    </div>
</div>

<div id="modal-container-GenLanguageEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenLanguageEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenLanguageDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenLanguageDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenLanguage/LandingGenLanguage.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenLanguage/NewGenLanguage.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenLanguage/EditGenLanguage.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenLanguage/DetailGenLanguage.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



