
@{
    ViewBag.Title = KAF.MsgContainer._arms_disbursementinfo.arms_disbursementinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("ArmsDisbursementInfo/ArmsDisbursementInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewArmsDisbursementInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="ArmsDisbursementInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._arms_disbursementinfo.issuenote</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-ArmsDisbursementInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcArmsDisbursementInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-ArmsDisbursementInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcArmsDisbursementInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-ArmsDisbursementInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcArmsDisbursementInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/ArmsDisbursementInfo/LandingArmsDisbursementInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/ArmsDisbursementInfo/NewArmsDisbursementInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/ArmsDisbursementInfo/EditArmsDisbursementInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/ArmsDisbursementInfo/DetailArmsDisbursementInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



