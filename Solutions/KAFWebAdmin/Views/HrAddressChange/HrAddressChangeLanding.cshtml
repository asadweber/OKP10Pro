
@{
    ViewBag.Title = KAF.MsgContainer._hr_addresschange.hr_addresschangeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrAddressChange/HrAddressChangeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrAddressChange">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrAddressChangeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_addresschange.kwblockno</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrAddressChangeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrAddressChangeNew">
        </div>
    </div>
</div>

<div id="modal-container-HrAddressChangeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrAddressChangeEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrAddressChangeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrAddressChangeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrAddressChange/LandingHrAddressChange.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrAddressChange/NewHrAddressChange.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrAddressChange/EditHrAddressChange.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrAddressChange/DetailHrAddressChange.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



