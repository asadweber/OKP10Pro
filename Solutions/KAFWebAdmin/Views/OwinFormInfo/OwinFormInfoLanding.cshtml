
@{
    ViewBag.Title = KAF.MsgContainer._owin_forminfo.owin_forminfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinFormInfo/OwinFormInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinFormInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinFormInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._owin_forminfo.formname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinFormInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinFormInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinFormInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormInfo/LandingOwinFormInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormInfo/NewOwinFormInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormInfo/EditOwinFormInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormInfo/DetailOwinFormInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



