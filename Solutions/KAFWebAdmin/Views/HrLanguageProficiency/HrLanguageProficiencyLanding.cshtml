
@{
    ViewBag.Title = KAF.MsgContainer._hr_languageproficiency.hr_languageproficiencyList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrLanguageProficiency/HrLanguageProficiencyNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrLanguageProficiency">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrLanguageProficiencyDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_languageproficiency.remarks</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrLanguageProficiencyNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrLanguageProficiencyNew">
        </div>
    </div>
</div>

<div id="modal-container-HrLanguageProficiencyEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrLanguageProficiencyEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrLanguageProficiencyDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrLanguageProficiencyDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrLanguageProficiency/LandingHrLanguageProficiency.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrLanguageProficiency/NewHrLanguageProficiency.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrLanguageProficiency/EditHrLanguageProficiency.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrLanguageProficiency/DetailHrLanguageProficiency.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



