
@{
    ViewBag.Title = KAF.MsgContainer._hr_residentvisa.hr_residentvisaList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrResidentVisa/HrResidentVisaNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrResidentVisa">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrResidentVisaDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_residentvisa.residencynumber</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrResidentVisaNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrResidentVisaNew">
        </div>
    </div>
</div>

<div id="modal-container-HrResidentVisaEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrResidentVisaEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrResidentVisaDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrResidentVisaDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrResidentVisa/LandingHrResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrResidentVisa/NewHrResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrResidentVisa/EditHrResidentVisa.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrResidentVisa/DetailHrResidentVisa.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



