
@{
    ViewBag.Title = KAF.MsgContainer._gen_religion.religionList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenReligion/GenReligionNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenReligion">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenReligionDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_religion.religion</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenReligionNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenReligionNew">
        </div>
    </div>
</div>

<div id="modal-container-GenReligionEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenReligionEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenReligionDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenReligionDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenReligion/LandingGenReligion.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenReligion/NewGenReligion.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenReligion/EditGenReligion.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenReligion/DetailGenReligion.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



