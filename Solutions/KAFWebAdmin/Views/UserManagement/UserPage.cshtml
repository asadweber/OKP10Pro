@{
    ViewBag.Title = "User Load";
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}


<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary panel-default list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                <button type="submit" class="btn btn-primary btn-md" data-url="@PageData["baseUrl"]@Url.Action("CreateUser", "UserManagement")" id="btnCreateUser">
                    <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._Common._btnAdd
                </button>

            </div>
            <div class="panel-body table-responsive">
                <table class="table table-striped table-bordered dataTables table-hover display" id="TableId" cellspacing="0" align="center">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._owin_user.loweredusername</th>
                            <th>@KAF.MsgContainer._Login.username</th>
                            <th>@KAF.MsgContainer._Login.mobilenumber</th>
                            <th>@KAF.MsgContainer._Login.emailaddress</th>
                            <th>@KAF.MsgContainer._CommonHtml._listAction</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-useredit" style="" class="modal fade hidden-print"  role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="usereditdatacontainer">
        </div>
    </div>
</div>

<div id="modal-container-userview" class="modal fade hidden-print"  role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="userviewdatacontainer">
        </div>
    </div>
</div>




@section scripts {
    <script src="~/DesignsAndScripts/PageWise/Security/UserManagement/LoadUser.js"></script>
    <script type="text/javascript" src="~/DesignsAndScripts/PageWise/Security/UserManagement/UserEdit.js"></script>
}