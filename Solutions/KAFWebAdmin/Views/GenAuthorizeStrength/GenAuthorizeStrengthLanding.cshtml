
@{
    ViewBag.Title = KAF.MsgContainer._gen_authorizestrength.authorizestrengthList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenAuthorizeStrength/GenAuthorizeStrengthNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenAuthorizeStrength">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenAuthorizeStrengthDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_authorizestrength.okpid</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenAuthorizeStrengthNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:60%!important">
        <div class="modal-content" id="mcGenAuthorizeStrengthNew">
        </div>
    </div>
</div>

<div id="modal-container-GenAuthorizeStrengthEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:60%!important">
        <div class="modal-content" id="mcGenAuthorizeStrengthEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenAuthorizeStrengthDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:60%!important">
        <div class="modal-content" id="mcGenAuthorizeStrengthDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenAuthorizeStrength/LandingGenAuthorizeStrength.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenAuthorizeStrength/NewGenAuthorizeStrength.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenAuthorizeStrength/EditGenAuthorizeStrength.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenAuthorizeStrength/DetailGenAuthorizeStrength.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



