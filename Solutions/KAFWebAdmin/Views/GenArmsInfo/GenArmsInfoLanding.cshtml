
@{
    ViewBag.Title = KAF.MsgContainer._gen_armsinfo.armsinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenArmsInfo/GenArmsInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenArmsInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenArmsInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._gen_armsinfo.armsname</th>
                            <th>@KAF.MsgContainer._gen_armsinfo.armscode</th>
                            <th>@KAF.MsgContainer._gen_armsinfo.ism16</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenArmsInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenArmsInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-GenArmsInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenArmsInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenArmsInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenArmsInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArmsInfo/LandingGenArmsInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArmsInfo/NewGenArmsInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArmsInfo/EditGenArmsInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArmsInfo/DetailGenArmsInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



