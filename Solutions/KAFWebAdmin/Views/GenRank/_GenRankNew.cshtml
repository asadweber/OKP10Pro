@model KAF.BusinessDataObjects.gen_rankEntity

<style>
        .form-horizontal .form-group {
            margin-right: 10px;
            margin-left: 10px;
        }
    </style>

@using (Ajax.BeginForm("", "", null, new AjaxOptions { HttpMethod = "Post", OnSuccess = "", LoadingElementId = "divProgressBar" }, new { @id = "frmGen_RankNew", @class = "form-horizontal" }))
{
    HtmlHelper.UnobtrusiveJavaScriptEnabled = true;
    HtmlHelper.ClientValidationEnabled = true;
    @Html.AntiForgeryToken()
    
    <div class="panel panel-primary list-panel" id="list-panel">
        <div class="panel-heading list-panel-heading">
            @KAF.MsgContainer._gen_rank.rankCreate
        </div>
        
        <div class="panel-body">
            <div class="modal-body">
                <div class="form-horizontal container">
                    <div class="row">
                        @Html.HiddenFor(m => m.rankid)
                    </div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.rankname, htmlAttributes: new { @class = "labelNormal required " })
								 @Html.EditorFor(model => model.rankname, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.rankname, "", new { @class = "text-danger" })
							</div>
						</div>
					</div>
					 <div class="row ">
						 <div class="col-md-6">
                             <div class="form-group">
                                 @Html.LabelFor(model => model.ranktypeid, htmlAttributes: new { @class = "labelNormal   required " })
                                 @*@{Html.RenderAction("LoadNoneSearch", "Common", new { area = "", selectid = "ranktypeid", minimumInputLength = 0, delay = 250, preloaded = ViewBag.preNone, multiple = false, isReadOnly = false, IsRequired = true });}*@
                                 @{Html.RenderAction("LoadComboBox", "DataCache", new { EntityName = KAF.CustomHelper.HelperClasses.clsDataCache.gen_ranktypeEntity[0].ToString(), selectid = "ranktypeid", minimumInputLength = 0, delay = 250, preloaded = ViewBag.pregen_bank, multiple = false, isReadOnly = false, IsRequired = true });}

                             </div>
						</div>
						 <div class="col-md-6">
                             <div class="form-group">
                                 @Html.LabelFor(model => model.countryid, htmlAttributes: new { @class = "labelNormal   required " })
                                 @* @{Html.RenderAction("LoadNoneSearch", "Common", new { area = "", selectid = "countryid", minimumInputLength = 0, delay = 250, preloaded = ViewBag.preNone, multiple = false, isReadOnly = false, IsRequired = true });}*@
                                 @{Html.RenderAction("LoadCountrySearch", "Common", new { area = "", selectid = "countryid", minimumInputLength = 0, delay = 250, preloaded = ViewBag.preNone, multiple = false, isReadOnly = false, IsRequired = true });}


                             </div>
						</div>
					</div>
					 <div class="row ">
					</div>
					 <div class="row ">
						 <div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.comments, htmlAttributes: new { @class = "labelNormal  " })
								 @Html.EditorFor(model => model.comments, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.comments, "", new { @class = "text-danger" })
							</div>
						</div>
						 @*<div class="col-md-6">
							 <div class="form-group">
								 @Html.LabelFor(model => model.remarks, htmlAttributes: new { @class = "labelNormal  " })
								 @Html.EditorFor(model => model.remarks, new { htmlAttributes = new {   @class = " form-control" } })
								 @Html.ValidationMessageFor(model => model.remarks, "", new { @class = "text-danger" })
							</div>
						</div>*@
					</div>
                    <div class="btn-toolbar">
                        <button type="submit" id="btnSaveGenRank" class="btn btn-primary btn-md btnSaveGenRank"><i class="fa fa-save"> </i> @KAF.MsgContainer._Common._btnSave</button>
                        <button type="submit" id="btnModalCloseNew" class="btn btn-danger btn-md cancel"><i class="fa fa-arrow-circle-left"> </i> @KAF.MsgContainer._Common._btnCancel</button>
                    </div> 
                </div>
            </div>
        </div>
    </div>
}


<script>
//filecontrol addons





 $j('.dateonly').datetimepicker(
        {
            // format: 'L'
            format: 'DD-MM-YYYY'
        });
</script>




