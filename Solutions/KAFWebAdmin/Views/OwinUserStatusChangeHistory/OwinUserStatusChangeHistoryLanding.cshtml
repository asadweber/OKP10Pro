
@{
    ViewBag.Title = KAF.MsgContainer._owin_userstatuschangehistory.owin_userstatuschangehistoryList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinUserStatusChangeHistory/OwinUserStatusChangeHistoryNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinUserStatusChangeHistory">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinUserStatusChangeHistoryDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._owin_userstatuschangehistory.userid</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinUserStatusChangeHistoryNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserStatusChangeHistoryNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinUserStatusChangeHistoryEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserStatusChangeHistoryEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinUserStatusChangeHistoryDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserStatusChangeHistoryDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserStatusChangeHistory/LandingOwinUserStatusChangeHistory.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserStatusChangeHistory/NewOwinUserStatusChangeHistory.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserStatusChangeHistory/EditOwinUserStatusChangeHistory.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserStatusChangeHistory/DetailOwinUserStatusChangeHistory.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



