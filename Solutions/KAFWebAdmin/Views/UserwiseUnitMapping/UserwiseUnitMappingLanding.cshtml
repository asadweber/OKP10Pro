
@{
    ViewBag.Title = "User wise Unit Mapping";
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    //PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("UserwiseUnitMapping/UserwiseUnitMappingNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md" id="btnNewUserwiseUnitMapping">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="UserwiseUnitMappingDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._owin_userunitmap.UserID</th>
                            <th>@KAF.MsgContainer._owin_userunitmap.RoleID</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-UserwiseUnitMappingNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcUserwiseUnitMappingNew">
        </div>
    </div>
</div>

<div id="modal-container-UserwiseUnitMappingEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcUserwiseUnitMappingEdit">
        </div>
    </div>
</div>

<div id="modal-container-UserwiseUnitMappingDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcUserwiseUnitMappingDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/UserwiseUnitMapping/LandingUserwiseUnitMapping.js"></script>
    <script src="~/DesignsAndScripts/PageWise/UserwiseUnitMapping/NewUserwiseUnitMapping.js"></script>
    <script src="~/DesignsAndScripts/PageWise/UserwiseUnitMapping/EditUserwiseUnitMapping.js"></script>
    <script src="~/DesignsAndScripts/PageWise/UserwiseUnitMapping/DetailUserwiseUnitMapping.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



