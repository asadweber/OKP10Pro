
@{
    ViewBag.Title = KAF.MsgContainer._hr_newdemanddetl.hr_newdemanddetlList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrNewDemandDetl/HrNewDemandDetlNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrNewDemandDetl">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrNewDemandDetlDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_newdemanddetl.remarks</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandDetlNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrNewDemandDetlNew">
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandDetlEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrNewDemandDetlEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandDetlDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrNewDemandDetlDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrNewDemandDetl/LandingHrNewDemandDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrNewDemandDetl/NewHrNewDemandDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrNewDemandDetl/EditHrNewDemandDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrNewDemandDetl/DetailHrNewDemandDetl.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



