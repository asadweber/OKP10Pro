
@{
    ViewBag.Title = KAF.MsgContainer._hr_newdemand.newdemandList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrNewDemand/HrNewDemandNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrNewDemand">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrNewDemandDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._hr_newdemand.demandletterno</th>
                            <th>@KAF.MsgContainer._hr_newdemand.demandletterdate</th>
                            <th>Total Person</th>
                            <th>Status</th>
                            <th style="width:320px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrNewDemandNew">
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrNewDemandEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrNewDemandDetail" class="modal fade hidden-print reportmodal" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcHrNewDemandDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrNewDemand/LandingHrNewDemand.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrNewDemand/NewHrNewDemand.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrNewDemand/EditHrNewDemand.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrNewDemand/DetailHrNewDemand.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



