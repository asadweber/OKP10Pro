
@{
    ViewBag.Title = KAF.MsgContainer._hr_flightinfodetl.hr_flightinfodetlList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrFlightInfoDetl/HrFlightInfoDetlNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrFlightInfoDetl">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrFlightInfoDetlDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_flightinfodetl.remarks</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrFlightInfoDetlNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFlightInfoDetlNew">
        </div>
    </div>
</div>

<div id="modal-container-HrFlightInfoDetlEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFlightInfoDetlEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrFlightInfoDetlDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFlightInfoDetlDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFlightInfoDetl/LandingHrFlightInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFlightInfoDetl/NewHrFlightInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFlightInfoDetl/EditHrFlightInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFlightInfoDetl/DetailHrFlightInfoDetl.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



