
@{
    ViewBag.Title = KAF.MsgContainer._hr_bankaccountinfo.hr_bankaccountinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrBankAccountInfo/HrBankAccountInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrBankAccountInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrBankAccountInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_bankaccountinfo.accountno</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrBankAccountInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrBankAccountInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrBankAccountInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrBankAccountInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrBankAccountInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrBankAccountInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrBankAccountInfo/LandingHrBankAccountInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrBankAccountInfo/NewHrBankAccountInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrBankAccountInfo/EditHrBankAccountInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrBankAccountInfo/DetailHrBankAccountInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



