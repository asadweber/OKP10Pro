
@{
    ViewBag.Title = KAF.MsgContainer._script_table_bmc.script_table_bmcList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("SCRIPTTABLEBMC/SCRIPTTABLEBMCNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewSCRIPTTABLEBMC">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="SCRIPTTABLEBMCDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._script_table_bmc.tablename</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTTABLEBMCNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTTABLEBMCNew">
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTTABLEBMCEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTTABLEBMCEdit">
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTTABLEBMCDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTTABLEBMCDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTTABLEBMC/LandingSCRIPTTABLEBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTTABLEBMC/NewSCRIPTTABLEBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTTABLEBMC/EditSCRIPTTABLEBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTTABLEBMC/DetailSCRIPTTABLEBMC.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



