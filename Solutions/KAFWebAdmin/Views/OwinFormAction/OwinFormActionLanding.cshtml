
@{
    ViewBag.Title = KAF.MsgContainer._owin_formaction.owin_formactionList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinFormAction/OwinFormActionNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinFormAction">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinFormActionDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._owin_formaction.actionname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinFormActionNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormActionNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinFormActionEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormActionEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinFormActionDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinFormActionDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormAction/LandingOwinFormAction.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormAction/NewOwinFormAction.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormAction/EditOwinFormAction.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinFormAction/DetailOwinFormAction.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



