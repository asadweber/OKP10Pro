
@{
    ViewBag.Title = KAF.MsgContainer._gen_company.companyList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenCompany/GenCompanyNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenCompany">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenCompanyDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_company.name</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenCompanyNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenCompanyNew">
        </div>
    </div>
</div>

<div id="modal-container-GenCompanyEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenCompanyEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenCompanyDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenCompanyDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCompany/LandingGenCompany.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCompany/NewGenCompany.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCompany/EditGenCompany.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCompany/DetailGenCompany.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



