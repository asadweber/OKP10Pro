
@{
    ViewBag.Title = KAF.MsgContainer._owin_role.owin_roleList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinRole/OwinRoleNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinRole">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinRoleDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._owin_role.rolename</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinRoleNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRoleNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinRoleEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRoleEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinRoleDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRoleDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRole/LandingOwinRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRole/NewOwinRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRole/EditOwinRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRole/DetailOwinRole.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



