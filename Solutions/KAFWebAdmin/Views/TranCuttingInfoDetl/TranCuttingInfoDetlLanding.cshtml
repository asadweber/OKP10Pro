
@{
    ViewBag.Title = KAF.MsgContainer._tran_cuttinginfodetl.cuttinginfodetlList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("TranCuttingInfoDetl/TranCuttingInfoDetlNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewTranCuttingInfoDetl">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="TranCuttingInfoDetlDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._tran_cuttinginfodetl.remarks</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-TranCuttingInfoDetlNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcTranCuttingInfoDetlNew">
        </div>
    </div>
</div>

<div id="modal-container-TranCuttingInfoDetlEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcTranCuttingInfoDetlEdit">
        </div>
    </div>
</div>

<div id="modal-container-TranCuttingInfoDetlDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcTranCuttingInfoDetlDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/TranCuttingInfoDetl/LandingTranCuttingInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/TranCuttingInfoDetl/NewTranCuttingInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/TranCuttingInfoDetl/EditTranCuttingInfoDetl.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/TranCuttingInfoDetl/DetailTranCuttingInfoDetl.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



