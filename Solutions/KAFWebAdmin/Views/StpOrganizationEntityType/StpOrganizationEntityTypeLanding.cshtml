
@{
    ViewBag.Title = KAF.MsgContainer._stp_organizationentitytype.organizationentitytypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("StpOrganizationEntityType/StpOrganizationEntityTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewStpOrganizationEntityType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="StpOrganizationEntityTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._stp_organizationentitytype.entirytypecode</th>
                            <th>@KAF.MsgContainer._stp_organizationentitytype.entirytype</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntityType/LandingStpOrganizationEntityType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntityType/NewStpOrganizationEntityType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntityType/EditStpOrganizationEntityType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntityType/DetailStpOrganizationEntityType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



