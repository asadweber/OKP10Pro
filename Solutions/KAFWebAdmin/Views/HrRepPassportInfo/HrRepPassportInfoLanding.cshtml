
@{
    ViewBag.Title = KAF.MsgContainer._hr_reppassportinfo.reppassportinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrRepPassportInfo/HrRepPassportInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrRepPassportInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrRepPassportInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._hr_reppassportinfo.passportletterno</th>
                            <th>@KAF.MsgContainer._hr_reppassportinfo.passportrecvdate</th>
                            <th>@KAF.MsgContainer._hr_reppassportinfo.passportletterdate</th>
                            <th>Total Person</th>
                            <th>Status</th>

                            <th style="width:350px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrRepPassportInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrRepPassportInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrRepPassportInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrRepPassportInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrRepPassportInfoDetail" class="modal fade hidden-print reportmodal" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrRepPassportInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrRepPassportInfo/LandingHrRepPassportInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrRepPassportInfo/NewHrRepPassportInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrRepPassportInfo/EditHrRepPassportInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrRepPassportInfo/DetailHrRepPassportInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



