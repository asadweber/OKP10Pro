
@{
    ViewBag.Title = KAF.MsgContainer._gen_birthdoctype.gen_birthdoctypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenBirthDocType/GenBirthDocTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenBirthDocType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenBirthDocTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_birthdoctype.birthdoctype</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenBirthDocTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBirthDocTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-GenBirthDocTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBirthDocTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenBirthDocTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBirthDocTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBirthDocType/LandingGenBirthDocType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBirthDocType/NewGenBirthDocType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBirthDocType/EditGenBirthDocType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBirthDocType/DetailGenBirthDocType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



