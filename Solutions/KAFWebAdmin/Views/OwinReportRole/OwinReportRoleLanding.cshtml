
@{
    ViewBag.Title = KAF.MsgContainer._owin_reportrole.reportroleList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinReportRole/OwinReportRoleNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinReportRole">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinReportRoleDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._owin_reportrole.reportrolename</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinReportRoleNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportRoleNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinReportRoleEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportRoleEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinReportRoleDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportRoleDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/Security/OwinReportRole/LandingOwinReportRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/Security/OwinReportRole/NewOwinReportRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/Security/OwinReportRole/EditOwinReportRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/Security/OwinReportRole/DetailOwinReportRole.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



