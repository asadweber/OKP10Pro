
@{
    ViewBag.Title = KAF.MsgContainer._owin_rolepermission.owin_rolepermissionList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinRolePermission/OwinRolePermissionNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinRolePermission">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinRolePermissionDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinRolePermissionNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRolePermissionNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinRolePermissionEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRolePermissionEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinRolePermissionDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinRolePermissionDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRolePermission/LandingOwinRolePermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRolePermission/NewOwinRolePermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRolePermission/EditOwinRolePermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinRolePermission/DetailOwinRolePermission.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



