
@{
    ViewBag.Title = KAF.MsgContainer._owin_userrole.owin_userroleList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinUserRole/OwinUserRoleNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinUserRole">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinUserRoleDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinUserRoleNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserRoleNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinUserRoleEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserRoleEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinUserRoleDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinUserRoleDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserRole/LandingOwinUserRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserRole/NewOwinUserRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserRole/EditOwinUserRole.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinUserRole/DetailOwinUserRole.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



