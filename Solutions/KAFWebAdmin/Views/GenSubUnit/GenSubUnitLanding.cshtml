
@{
    ViewBag.Title = KAF.MsgContainer._gen_subunit.subunitList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenSubUnit/GenSubUnitNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenSubUnit">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenSubUnitDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_subunit.subunit</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenSubUnitNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenSubUnitNew">
        </div>
    </div>
</div>

<div id="modal-container-GenSubUnitEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenSubUnitEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenSubUnitDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcGenSubUnitDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenSubUnit/LandingGenSubUnit.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenSubUnit/NewGenSubUnit.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenSubUnit/EditGenSubUnit.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenSubUnit/DetailGenSubUnit.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



