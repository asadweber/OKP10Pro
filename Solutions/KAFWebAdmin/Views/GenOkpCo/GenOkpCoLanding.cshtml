
@{
    ViewBag.Title = KAF.MsgContainer._gen_okpco.okpcoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenOkpCo/GenOkpCoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenOkpCo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenOkpCoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>Okp Name</th>
                            <th>From Date</th>
                            <th>To Date</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenOkpCoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenOkpCoNew">
        </div>
    </div>
</div>

<div id="modal-container-GenOkpCoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenOkpCoEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenOkpCoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenOkpCoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenOkpCo/LandingGenOkpCo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenOkpCo/NewGenOkpCo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenOkpCo/EditGenOkpCo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenOkpCo/DetailGenOkpCo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



