
@{
    ViewBag.Title = KAF.MsgContainer._gen_country.countryList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenCountry/GenCountryNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenCountry">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenCountryDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_country.countryname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenCountryNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCountryNew">
        </div>
    </div>
</div>

<div id="modal-container-GenCountryEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCountryEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenCountryDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCountryDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCountry/LandingGenCountry.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCountry/NewGenCountry.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCountry/EditGenCountry.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCountry/DetailGenCountry.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



