@{
    ViewBag.Title = "Report Panel";
}
<style>
    .detailheading {
        background-color: darkgray !important;
    }

    .buttonpanelcenter {
        text-align: center;
    }

    .form-horizontal .form-group {
        margin-right: 10px;
        margin-left: 10px;
    }

    #colsortable ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        list-style-type: none;
    }

    #colsortable {
        margin: 0;
        padding: 0;
    }

        #colsortable li {
            margin: 5px;
            padding: 0px;
            list-style-type: none;
            width: 100%;
        }

    .sortingasc {
        background-image: url(../DesignsAndScripts/Scripts/plugins/datatables/images/sort_asc.png);
    }

    .sortingdesc {
        background-image: url(../DesignsAndScripts/Scripts/plugins/datatables/images/sort_desc.png);
    }
</style>
@{
    if (System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant().ToLower() == "ar-kw")
    {
        <style>
            .actionbuttonletright {
                text-align: left;
            }
        </style>
    }
    else
    {
        <style>
            .actionbuttonletright {
                text-align: right;
            }
        </style>
    }
}


<script src="~/ckeditor/ckeditor.js"></script>
<img src="~/DesignsAndScripts/Scripts/plugins/datatables/images/sort_desc.png" />
