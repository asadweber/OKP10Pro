@section Scripts {
    <script>
        function ghh() {

        }

        $(document).ready(function () {

            if ($("#hddokpid").val() != "") {
                $("#okpid").val($("#hddokpid").val()).trigger('change');
                $("#okpid").prop("disabled", true);
            }

            $.each($(".chkcolumn"), function (val, element) {

                if ($(element).prop("checked") == true) {
                    $('#colsortable').append("<li  class='ui-state-default'  colname=" + encodeURIComponent($(element).val()) + ">" + $(element).next().text() + "</li>");
                }
                else {
                    $("#colsortable li[colname=\"" + encodeURIComponent($(element).val()) + "\"]").remove();
                }

                $("#colsortable").sortable({
                    revert: true,
                    stop: function (event, ui) {
                        if (!ui.item.data('tag') && !ui.item.data('handle')) {
                            ui.item.data('tag', true);
                            //ui.item.fadeTo(400, 0.1);
                        }
                    }
                });

                $("ul, li").disableSelection();
            })

            $(".chkcolumn").click(function () {
                if ($(this).prop("checked") == true) {
                    $('#colsortable').append("<li  class='ui-state-default'  colname=" + encodeURIComponent($(this).val()) + ">" + $(this).next().text() + "</li>");
                }
                else {
                    $("#colsortable li[colname=\"" + encodeURIComponent($(this).val()) + "\"]").remove();
                }

                $("#colsortable").sortable({
                    revert: true,
                    stop: function (event, ui) {
                        if (!ui.item.data('tag') && !ui.item.data('handle')) {
                            ui.item.data('tag', true);
                            //ui.item.fadeTo(400, 0.1);
                        }
                    }
                });

                $("ul, li").disableSelection();
            });
            $("#ranktypeidbd").change(function () {
                LoadBDRankByRankType($("#ranktypeidbd").val(), 'rankidbd');

            });
            $("#ranktypeidkw").change(function () {

                LoadKWRankByRanktypeID($("#ranktypeidkw").val(), 'rankidkw');
            });

            $("#btnPrint").click(function () {
                $("#divReport").printThis({
                    importCSS: true,
                    loadCSS: "~/DesignsAndScripts/Styles/bootstrap_min.css",
                    header: "<h1>Custom Report</h1>"
                });
            });
            $("#btnExport").click(function () {
                $("#RegBasicInfoDt").table2excel({
                    filename: "DynamicReport" + new Date().toLocaleDateString().replaceAll('/', '') + new Date().toLocaleTimeString().replaceAll(' ', '').replaceAll(':', '')+".xls"
                });
            });
            $('#btnSearch').click(function () {

                var strcols = "", strwhere = "";

                $.each($("#colsortable li"), function (val, element) {

                    strcols += decodeURIComponent($(element).attr('colname')) + ',';
                })


                if ($("#organizationkey").val() != null && $("#organizationkey").val() != "")
                    strwhere += " hr_svcinfo.organizationkey=" + $("#organizationkey").val() + " AND ";
                if ($("#entitykey").val() != null && $("#entitykey").val() != "")
                    strwhere += " stp_organizationentity.EntityKey=" + $("#entitykey").val() + " AND ";

                if ($("#armsid").val() != null && $("#armsid").val() != "")
                    strwhere += "  gen_arms.armsid=" + $("#armsid").val() + " AND ";

                if ($("#okpid").val() != null && $("#okpid").val() != "")
                    strwhere += " gen_okp.okpid=" + $("#okpid").val() + " AND ";

                if ($("#Group").val() != null && $("#Group").val() != "")
                    strwhere += " hr_svcinfo.groupid=" + $("#Group").val() + " AND ";

                if ($("#ranktypeidkw").val() != null && $("#ranktypeidkw").val() != "")
                    strwhere += " rnk_kw.RankTypeID=" + $("#ranktypeidkw").val() + " AND ";

                if ($("#rankidkw").val() != null && $("#rankidkw").val() != "")
                    strwhere += " Hr_SvcInfo.RankIDKW=" + $("#rankidkw").val() + " AND ";

