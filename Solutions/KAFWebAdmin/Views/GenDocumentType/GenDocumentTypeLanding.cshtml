
@{
    ViewBag.Title = KAF.MsgContainer._gen_documenttype.documenttypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenDocumentType/GenDocumentTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenDocumentType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenDocumentTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_documenttype.doctypename</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenDocumentTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenDocumentTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-GenDocumentTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenDocumentTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenDocumentTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenDocumentTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenDocumentType/LandingGenDocumentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenDocumentType/NewGenDocumentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenDocumentType/EditGenDocumentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenDocumentType/DetailGenDocumentType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



