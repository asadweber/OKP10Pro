
@{
    ViewBag.Title = KAF.MsgContainer._owin_reportpermission.owin_reportpermissionList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("OwinReportPermission/OwinReportPermissionNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewOwinReportPermission">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="OwinReportPermissionDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-OwinReportPermissionNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportPermissionNew">
        </div>
    </div>
</div>

<div id="modal-container-OwinReportPermissionEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportPermissionEdit">
        </div>
    </div>
</div>

<div id="modal-container-OwinReportPermissionDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcOwinReportPermissionDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinReportPermission/LandingOwinReportPermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinReportPermission/NewOwinReportPermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinReportPermission/EditOwinReportPermission.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/OwinReportPermission/DetailOwinReportPermission.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



