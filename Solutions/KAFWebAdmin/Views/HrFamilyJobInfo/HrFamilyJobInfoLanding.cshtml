
@{
    ViewBag.Title = KAF.MsgContainer._hr_familyjobinfo.hr_familyjobinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("HrFamilyJobInfo/HrFamilyJobInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewHrFamilyJobInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="HrFamilyJobInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._hr_familyjobinfo.organization</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyJobInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyJobInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyJobInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyJobInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrFamilyJobInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrFamilyJobInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyJobInfo/LandingHrFamilyJobInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyJobInfo/NewHrFamilyJobInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyJobInfo/EditHrFamilyJobInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/HrFamilyJobInfo/DetailHrFamilyJobInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



