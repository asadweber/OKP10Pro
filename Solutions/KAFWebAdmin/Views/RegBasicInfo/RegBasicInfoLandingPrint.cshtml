
@{
    ViewBag.Title = KAF.MsgContainer._reg_basicinfo.basicinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">

                <button type="button" class="btn btn-primary btn-md" id="btnPrint">
                    <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> Print
                </button>

            </div>
            <div class="panel-body">
                <table id="RegBasicInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._reg_basicinfo.fullname</th>
                            <th>@KAF.MsgContainer._reg_basicinfo.studentid</th>
                            <th>QR Code</th>

                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>



@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/RegBasicInfo/LandingRegBasicInfoPrint.js"></script>
   
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



