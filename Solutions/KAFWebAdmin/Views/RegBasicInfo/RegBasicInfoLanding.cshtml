
@{
    ViewBag.Title = KAF.MsgContainer._reg_basicinfo.basicinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("RegBasicInfo/RegBasicInfoNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewRegBasicInfo">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="RegBasicInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._reg_basicinfo.fullname</th>
                            <th>@KAF.MsgContainer._reg_basicinfo.studentid</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-RegBasicInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcRegBasicInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-RegBasicInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcRegBasicInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-RegBasicInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:50%!important">
        <div class="modal-content" id="mcRegBasicInfoDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/RegBasicInfo/LandingRegBasicInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/RegBasicInfo/NewRegBasicInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/RegBasicInfo/EditRegBasicInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/RegBasicInfo/DetailRegBasicInfo.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



