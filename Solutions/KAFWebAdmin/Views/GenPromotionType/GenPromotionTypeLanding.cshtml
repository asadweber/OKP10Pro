
@{
    ViewBag.Title = KAF.MsgContainer._gen_promotiontype.promotiontypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenPromotionType/GenPromotionTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenPromotionType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenPromotionTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_promotiontype.promotiontype</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenPromotionTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenPromotionTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-GenPromotionTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenPromotionTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenPromotionTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenPromotionTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPromotionType/LandingGenPromotionType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPromotionType/NewGenPromotionType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPromotionType/EditGenPromotionType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenPromotionType/DetailGenPromotionType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



