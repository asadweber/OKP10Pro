
@{
    ViewBag.Title = KAF.MsgContainer._gen_bank.bankList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenBank/GenBankNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenBank">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenBankDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_bank.bankname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenBankNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBankNew">
        </div>
    </div>
</div>

<div id="modal-container-GenBankEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBankEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenBankDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenBankDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBank/LandingGenBank.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBank/NewGenBank.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBank/EditGenBank.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenBank/DetailGenBank.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



