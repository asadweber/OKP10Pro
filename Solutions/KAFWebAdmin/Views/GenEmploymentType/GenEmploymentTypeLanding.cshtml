
@{
    ViewBag.Title = KAF.MsgContainer._gen_employmenttype.gen_employmenttypeList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenEmploymentType/GenEmploymentTypeNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenEmploymentType">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenEmploymentTypeDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_employmenttype.employmenttype</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenEmploymentTypeNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenEmploymentTypeNew">
        </div>
    </div>
</div>

<div id="modal-container-GenEmploymentTypeEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenEmploymentTypeEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenEmploymentTypeDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenEmploymentTypeDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenEmploymentType/LandingGenEmploymentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenEmploymentType/NewGenEmploymentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenEmploymentType/EditGenEmploymentType.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenEmploymentType/DetailGenEmploymentType.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



