

@{
    ViewBag.Title = KAF.MsgContainer._stp_organizationentity.organizationentityList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission(HttpContext.Current.User.Identity as System.Security.Claims.ClaimsIdentity, "StpOrganizationEntity/StpOrganizationEntityCreate"))
                    {
                        <button type="submit" class="btn btn-primary btn-md" data-url="@PageData["baseUrl"]@Url.Action("StpOrganizationEntityCreate", "StpOrganizationEntity")" id="btnCreateStpOrganizationEntity">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>

            <div class="panel-body">
                <table id="StpOrganizationEntityDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._stp_organizationentity.entityname</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.organizationkey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.parentkey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.entirytypekey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.entitylevel</th>
                            @*<th>@KAF.MsgContainer._stp_organizationentity.seqfirstindex</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.seqfullindex</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.entitycpde</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.description</th>*@
                            <th>@KAF.MsgContainer._stp_organizationentity.isactive</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>


<div id="modal-container-StpOrganizationEntityedit" class="modal fade hidden-print" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" id="StpOrganizationEntityeditdatacontainer">
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityview" class="modal fade hidden-print" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" id="StpOrganizationEntityviewdatacontainer">
        </div>
    </div>
</div>


@*<input type="hidden" value="@PageData["baseUrl"]" id="txtBaseUrl" />*@

@*@Html.Action("AdvancedSearch")*@
@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/StpOrganizationEntity/LoadStpOrganizationEntity.js"></script>
    <script src="~/DesignsAndScripts/PageWise/StpOrganizationEntity/UpdateStpOrganizationEntity.js"></script>
}	
	