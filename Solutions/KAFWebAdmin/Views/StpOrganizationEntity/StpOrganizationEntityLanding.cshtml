
@{
    ViewBag.Title = KAF.MsgContainer._stp_organizationentity.organizationentityList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("StpOrganizationEntity/StpOrganizationEntityNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewStpOrganizationEntity">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="StpOrganizationEntityDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@KAF.MsgContainer._stp_organizationentity.entityname</th>
                            @*<th>@KAF.MsgContainer._stp_organizationentity.organizationkey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.parentkey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.entirytypekey</th>
                            <th>@KAF.MsgContainer._stp_organizationentity.entitylevel</th>*@
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityNew">
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityEdit">
        </div>
    </div>
</div>

<div id="modal-container-StpOrganizationEntityDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcStpOrganizationEntityDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntity/LandingStpOrganizationEntity.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntity/NewStpOrganizationEntity.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntity/EditStpOrganizationEntity.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/StpOrganizationEntity/DetailStpOrganizationEntity.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



