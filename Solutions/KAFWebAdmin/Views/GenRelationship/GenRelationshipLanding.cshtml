
@{
    ViewBag.Title = KAF.MsgContainer._gen_relationship.relationshipList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenRelationship/GenRelationshipNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenRelationship">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenRelationshipDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_relationship.relationshipname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenRelationshipNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenRelationshipNew">
        </div>
    </div>
</div>

<div id="modal-container-GenRelationshipEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenRelationshipEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenRelationshipDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenRelationshipDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenRelationship/LandingGenRelationship.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenRelationship/NewGenRelationship.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenRelationship/EditGenRelationship.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenRelationship/DetailGenRelationship.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



