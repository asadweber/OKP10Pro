
@{
    ViewBag.Title = KAF.MsgContainer._gen_arms.armsList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenArms/GenArmsNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenArms">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenArmsDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._gen_arms.armsname</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenArmsNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenArmsNew">
        </div>
    </div>
</div>

<div id="modal-container-GenArmsEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenArmsEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenArmsDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenArmsDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArms/LandingGenArms.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArms/NewGenArms.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArms/EditGenArms.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenArms/DetailGenArms.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



