
@{
    ViewBag.Title = "Currency List";
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("GenCurrencyExchageRate/GenCurrencyExchageRateNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewGenCurrencyExchageRate">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="GenCurrencyExchageRateDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>From Currency</th>
                            <th>To Currency</th>
                            <th>From Date</th>
                            <th>To Date</th>
                            <th>Rate</th>
                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-GenCurrencyExchageRateNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCurrencyExchageRateNew">
        </div>
    </div>
</div>

<div id="modal-container-GenCurrencyExchageRateEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCurrencyExchageRateEdit">
        </div>
    </div>
</div>

<div id="modal-container-GenCurrencyExchageRateDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcGenCurrencyExchageRateDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCurrencyExchageRate/LandingGenCurrencyExchageRate.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCurrencyExchageRate/NewGenCurrencyExchageRate.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCurrencyExchageRate/EditGenCurrencyExchageRate.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/GenCurrencyExchageRate/DetailGenCurrencyExchageRate.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



