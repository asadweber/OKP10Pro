@model KAF.BusinessDataObjects.hr_svcinfoEntity
@{
    ViewBag.Title = KAF.MsgContainer._hr_personalinfo.personalinfoList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}
<style>
    .basicDetails {
        margin-left: 15px;
        margin-right: 15px;
    }
</style>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            @*<div class="panel-heading list-panel-heading">
                    @{
                        KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();
                        if (objSecPanel.checkButtonPermission("HrPersonalInfo/HrPersonalInfoNew"))
                        {
                                <button type="submit" class="btn btn-primary btn-md btnNewHrPersonalInfo" id="btnNewHrPersonalInfo">
                                    <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                                </button>
                        }
                     }
                </div>*@
            <div class="panel panel-info">
                <div class="panel-heading">
                    Basic Details
                </div>
                <div class="panel-body">
                    <div class="row ">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.Label("Military No (KW)", htmlAttributes: new { @class = "labelNormal  required" })
                                @Html.EditorFor(model => model.militarynokw, new { htmlAttributes = new { @class = "required form-control" } })
                                @Html.ValidationMessageFor(model => model.militarynokw, "", new { @class = "text-danger" })
                            </div>
                        </div>

                    </div>
                    <div class="row ">
                        <div class="col-md-6">
                            <div class="form-group">
                                <div class="btn-toolbar align-center">
                                    <button type="submit" id="btnLandingSearch" class="btn btn-primary btn-md btnLandingSearch">
                                        <i class="fa fa-search"> </i>
                                        @KAF.MsgContainer._Common._btnSearch
                                    </button>

                                    <button type="submit" id="btnClearLandingSearch" class="btn btn-danger btn-md btnClearLandingSearch">
                                        <i class="fa fa-close"> </i> Clear
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="basicDetails">
                    <div class="row ">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.Label("Military No (BD)", htmlAttributes: new { @class = "labelNormal  " })
                                @Html.EditorFor(model => model.militarynobd, new { htmlAttributes = new { @readonly = "readonly", @class = " form-control" } })
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.civilid, htmlAttributes: new { @class = "labelNormal  " })
                                @Html.EditorFor(model => model.civilid, new { htmlAttributes = new { @readonly = "readonly", @class = "form-control" } })
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.passportno, htmlAttributes: new { @class = "labelNormal  " })
                                @Html.EditorFor(model => model.passportno, new { htmlAttributes = new { @readonly = "readonly", @class = " form-control" } })
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.fullname, htmlAttributes: new { @class = "labelNormal " })
                                @Html.EditorFor(model => model.fullname, new { htmlAttributes = new { @readonly = "readonly", @class = " form-control" } })
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-md-6">
                            <div class="form-group">
                                <div class="btn-toolbar align-center">
                                    @{
                                        KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();
                                        if (objSecPanel.checkButtonPermission("HrPersonalInfo/HrPersonalInfoNew"))
                                        {
                                            <button type="submit" class="btn btn-primary btn-md btnNewHrPersonalInfo" id="btnNewHrPersonalInfo">
                                                <span style="width:150px;" class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                                            </button>
                                        }
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*<div class="panel-body">



                </div>*@
            <div class="panel-body">
                <table id="HrPersonalInfoDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
                            <th>Military No (KW)</th>
                            <th>Status</th>
                            <th style="width:350px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-HrPersonalInfoNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrPersonalInfoNew">
        </div>
    </div>
</div>

<div id="modal-container-HrPersonalInfoEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrPersonalInfoEdit">
        </div>
    </div>
</div>

<div id="modal-container-HrPersonalInfoDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcHrPersonalInfoDetail">
        </div>
    </div>
</div>

<div id="modal-container-HrDocumentUploadLanding" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important; ">
        <div class="modal-content" id="mcHrDocumentUploadLanding" style="height: 600px !important">
        </div>
    </div>
</div>
@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrPersonalInfo/LandingHrPersonalInfo.js?p=002"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrPersonalInfo/NewHrPersonalInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrPersonalInfo/EditHrPersonalInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrPersonalInfo/DetailHrPersonalInfo.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDocumentUpload/LandingHrDocumentUpload.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDocumentUpload/NewHrDocumentUpload.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDocumentUpload/EditHrDocumentUpload.js"></script>
    <script src="~/DesignsAndScripts/PageWise/HRJS/HrDocumentUpload/DetailHrDocumentUpload.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';

        $('body').on('click', '#btnModalCloseLanding', function (event) {

            try {
                event.preventDefault();
                $('#mcHrDocumentUploadLanding').html('');
                $('#modal-container-HrDocumentUploadLanding').modal('hide');
            } catch (e) {
                $.alert({
                    title: _getCookieForLanguage("_informationTitle"),
                    content: e.message,
                    type: 'red'
                });
            }
        });
    </script>
}




