
@{
    ViewBag.Title = KAF.MsgContainer._script_ins_bmc.script_ins_bmcList;
    Layout = "~/Views/Shared/_AdminLteLayout.cshtml";
    PageData["baseUrl"] = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                @{
                    KAF.CustomHelper.HelperClasses.clsSecurityPanel objSecPanel = new KAF.CustomHelper.HelperClasses.clsSecurityPanel();

                    if (objSecPanel.checkButtonPermission("SCRIPTINSBMC/SCRIPTINSBMCNew"))
                    {
                        <button type="submit" class="btn btn-primary btn-md"  id="btnNewSCRIPTINSBMC">
                            <span class="glyphicon glyphicon-new-window" aria-hidden="true"></span> @KAF.MsgContainer._CommonHtml.add
                        </button>
                    }
                }
            </div>
            <div class="panel-body">
                <table id="SCRIPTINSBMCDt" class="table table-striped table-bordered" style="width:100%;">
                    <thead>
                        <tr>
							 <th>@KAF.MsgContainer._script_ins_bmc.script</th>                            <th style="width:250px;"></th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTINSBMCNew" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTINSBMCNew">
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTINSBMCEdit" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTINSBMCEdit">
        </div>
    </div>
</div>

<div id="modal-container-SCRIPTINSBMCDetail" class="modal fade hidden-print" role="dialog">
    <div class="modal-dialog" style="width:90%!important">
        <div class="modal-content" id="mcSCRIPTINSBMCDetail">
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/ckeditor/ckeditor.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTINSBMC/LandingSCRIPTINSBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTINSBMC/NewSCRIPTINSBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTINSBMC/EditSCRIPTINSBMC.js"></script>
    <script src="~/DesignsAndScripts/PageWise/SetupJS/SCRIPTINSBMC/DetailSCRIPTINSBMC.js"></script>
    <script>
        var addlink = '@ViewBag.addlink';
    </script>
}



